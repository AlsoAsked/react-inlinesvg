{
  "name": "react-inlinesvg",
  "version": "0.4.2",
  "description": "An SVG loader for React",
  "repository": {
    "type": "git",
    "url": "git://github.com/matthewwithanm/react-inlinesvg.git"
  },
  "keywords": [
    "react-component",
    "react",
    "svg",
    "component"
  ],
  "author": "Matthew Dapena-Tretter <m@tthewwithanm.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/matthewwithanm/react-inlinesvg/issues"
  },
  "main": "./lib/index.js",
  "dependencies": {
    "httpplease": "^0.16",
    "once": "^1.3",
    "react-addons-shallow-compare": "^15.0"
  },
  "devDependencies": {
    "babel-core": "^6.7",
    "babel-eslint": "^6.0",
    "babel-plugin-add-module-exports": "^0.1",
    "babel-plugin-transform-decorators-legacy": "^1.3",
    "babel-plugin-transform-inline-environment-variables": "^6.5",
    "babel-preset-es2015": "^6.6",
    "babel-preset-react": "^6.5",
    "babel-preset-stage-1": "^6.5",
    "babel-register": "^6.7",
    "babelify": "^7.2.0",
    "browserify": "^13.0.0",
    "browserify-shim": "^3.8.12",
    "cors": "^2.2",
    "eslint": "^2.7",
    "eslint-config-airbnb": "^6.2",
    "eslint-plugin-jsdoc": "^2.3",
    "eslint-plugin-react": "^4.2",
    "expect": "^1.16",
    "gulp": "^3.9",
    "gulp-babel": "^6.1",
    "gulp-bump": "^2.1.0",
    "gulp-connect": "^3.2",
    "gulp-load-plugins": "^1.2",
    "gulp-rename": "^1.2",
    "gulp-util": "^3.0",
    "jsdom": "^8.4",
    "mocha": "^2.4",
    "react": "^15.0",
    "react-addons-test-utils": "^15.0",
    "react-dom": "^15.0",
    "vinyl-source-stream": "^1.1.0"
  },
  "peerDependencies": {
    "react": "^0.14 || ^15.0"
  },
  "scripts": {
    "build": "gulp test & (npm test && gulp build) && kill %1",
    "test": "./node_modules/.bin/mocha --compilers js:babel-register",
    "lint": "./node_modules/.bin/eslint -c ./.eslintrc $(find src -name '*.js')",
    "prepublish": "npm run build"
  },
  "babel": {
    "presets": [
      "es2015",
      "react",
      "stage-1"
    ],
    "plugins": [
      "add-module-exports",
      "transform-decorators-legacy"
    ]
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "ignore": "/bower_components/",
          "sourceMapRelative": "."
        }
      ],
      "browserify-shim"
    ]
  },
  "browserify-shim": {
    "react": "global:React"
  }
}
